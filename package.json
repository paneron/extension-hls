{
  "name": "@paneron/extension-hls",
  "version": "1.0.0-dev18",
  "description": "Paneron extension for SMART model editing",
  "main": "plugin.js",
  "repository": "https://github.com/paneron/extension-hls",
  "author": {
    "name": "Ribose Inc.",
    "email": "open.source@ribose.com"
  },
  "paneronExtension": {
    "title": "SMART",
    "iconURL": "https://www.glossarist.org/SymbolLoupe.png",
    "featured": true,
    "requiredHostAppVersion": "^1.0.0-beta18"
  },
  "scripts": {
    "build": "rm -rf compiled dist/*; mkdir -p compiled dist && tsc && babel compiled --out-dir dist && cp package.json dist/ && rsync -a --include '*/' --include '*.d.ts' --include '*.js.map' --exclude '*' compiled/ dist/",
    "buildwin": "rmdir /q /s compiled dist & mkdir compiled dist & tsc & xcopy /e /y compiled dist > nul & babel compiled --out-dir dist & copy package.json dist & md \"%appdata%\\Paneron\\plugins\\@riboseinc\\paneron-extension-hls\" & xcopy /e /y dist \"%appdata%\\Paneron\\plugins\\@riboseinc\\paneron-extension-hls\" > nul",
    "lint": "gts lint",
    "clean": "gts clean",
    "fix": "gts fix"
  },
  "files": [
    "README.adoc",
    "*.js",
    "*.js.map",
    "*.d.ts",
    "**/*.js",
    "**/*.js.map",
    "**/*.d.ts"
  ],
  "resolutions": {
    "@types/react": "16.9.49",
    "react": "16.14.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@blueprintjs/core": "3.44.1",
    "@blueprintjs/icons": "3.26.0",
    "@blueprintjs/popover2": "0.7.1",
    "@blueprintjs/select": "^3.17.0",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@react-three/drei": "^7.22.6",
    "@react-three/fiber": "^7.0.19",
    "@riboseinc/paneron-extension-glossarist": "1.0.0-dev34",
    "@riboseinc/paneron-extension-kit": "2.0.0-alpha23",
    "@types/node": "^14.11.2",
    "@types/react": "16.9.49",
    "@types/react-reconciler": "^0.26.4",
    "@types/three": "^0.134.0",
    "@types/webpack-env": "^1.16.0",
    "gts": "^3.1.0",
    "liquidjs": "^9.25.0",
    "react": "16.14.0",
    "react-flow-renderer": "^9.6.0",
    "react-mathjax2": "^0.0.2",
    "three": "^0.134.0",
    "three-stdlib": "^2.5.9",
    "typescript": "^4.1.3"
  },
  "license": "MIT"
}
